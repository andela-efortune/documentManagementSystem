{
  "name": "documentmanagementsystem",
  "version": "1.0.0",
  "description": "A system that manages documents, users and user roles.",
  "main": "sever.js",
  "scripts": {
    "start": "nodemon server.js --exec babel-node --presets es2015,stage-2",
    "test_db_migrate": "NODE_ENV=test sequelize db:migrate",
    "test": "NODE_ENV=test mocha --compilers js:babel-core/register test/**/*.spec.js",
    "cover":
      "rm -rf /coverage && node_modules/.bin/babel-node node_modules/.bin/babel-istanbul cover node_modules/.bin/_mocha test/**/*.spec.js",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela-efortune/documentManagementSystem.git"
  },
  "keywords": [
    "Document",
    "API",
    "system",
    "management"
  ],
  "author": "fortune",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andela-efortune/documentManagementSystem/issues"
  },
  "homepage": "https://github.com/andela-efortune/documentManagementSystem#readme",
  "devDependencies": {
    "sequelize": "^3.27.0"
  },
  "dependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-istanbul": "^0.11.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-2": "^6.18.0",
    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.15.2",
    "chai": "^3.5.0",
    "codeclimate-test-reporter": "^0.4.0",
    "coveralls": "^2.11.15",
    "eslint": "^3.11.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.1.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.6.0",
    "express": "^4.14.0",
    "faker": "^3.1.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "mocha-lcov-reporter": "^1.2.0",
    "morgan": "^1.7.0",
    "pg": "^6.1.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^3.27.0",
    "supertest": "^2.0.1"
  }
}
